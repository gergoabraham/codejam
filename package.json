{
  "name": "codejam",
  "version": "1.0.0",
  "author": "gergooo",
  "scripts": {
    "tdd-skeleton": "mocha --recursive ./skeletons/**/*spec.js --watch",
    "tdd2020-0": "mocha --recursive ./2020/0*/**/*spec.js --watch",
    "tdd2020-1b": "mocha --recursive ./2020/1B/**/*spec.js --watch --timeout=40000",
    "tdd2020-1c": "mocha --recursive ./2020/1C/**/*spec.js --watch --timeout=40000",
    "tdd2021-0": "mocha --recursive ./2021/0*/**/*spec.js --watch",
    "tdd2021-1a": "mocha --recursive ./2021/1A/**/*spec.js --watch",
    "tdd2021-1b": "mocha --recursive ./2021/1B/**/*spec.js --watch",
    "tdd2021-1c": "mocha --recursive ./2021/1C/**/*spec.js --watch",
    "tdd2022-0": "mocha --recursive ./2022/0*/**/*spec.js --watch",
    "tdd2022-1b": "mocha --recursive ./2022/1B/**/*spec.js --watch",
    "tdd2022-1c": "mocha --recursive ./2022/1C/**/*spec.js --watch",
    "test": "mocha --recursive ./**/*spec.js --forbid-only"
  },
  "devDependencies": {
    "chai": "^4.3.4",
    "eslint": "^7.23.0",
    "eslint-config-google": "^0.14.0",
    "mocha": "^8.3.2"
  },
  "mocha": {
    "recursive": true,
    "file": "./mocha-setup.js"
  }
}
