{
  "name": "codejam-2020",
  "version": "1.0.0",
  "author": "gergooo",
  "scripts": {
    "tdd-skeleton": "mocha --recursive ./skeletons/**/*spec.js --watch",
    "tdd0": "mocha --recursive ./0*/**/*spec.js --watch",
    "tdd1b": "mocha --recursive ./1B/**/*spec.js --watch --timeout=40000",
    "tdd1c": "mocha --recursive ./1C/**/*spec.js --watch --timeout=40000",
    "test": "mocha --recursive ./**/*spec.js --forbid-only"
  },
  "devDependencies": {
    "chai": "^4.3.4",
    "eslint": "^7.23.0",
    "eslint-config-google": "^0.14.0",
    "mocha": "^8.3.2"
  },
  "mocha": {
    "recursive": true,
    "file": "./mocha-setup.js"
  }
}
